{
  "version": 2,
  "projects": {
    "demo": {
      "root": "apps/demo/",
      "sourceRoot": "apps/demo/src",
      "projectType": "application",
      "prefix": "demo",
      "targets": {
        "build": {
          "executor": "@nativescript/nx:build",
          "options": {
            "noHmr": true,
            "production": true,
            "uglify": true,
            "release": true,
            "forDevice": true
          }
        },
        "ios": {
          "executor": "@nativescript/nx:build",
          "options": {
            "platform": "ios"
          }
        },
        "android": {
          "executor": "@nativescript/nx:build",
          "options": {
            "platform": "android"
          }
        },
        "clean": {
          "executor": "@nativescript/nx:build",
          "options": {
            "clean": true
          }
        }
      }
    },
    "demo-angular": {
      "root": "apps/demo-angular/",
      "sourceRoot": "apps/demo-angular/src",
      "projectType": "application",
      "prefix": "demo",
      "targets": {
        "build": {
          "executor": "@nativescript/nx:build",
          "options": {
            "noHmr": true,
            "production": true,
            "uglify": true,
            "release": true,
            "forDevice": true
          }
        },
        "ios": {
          "executor": "@nativescript/nx:build",
          "options": {
            "platform": "ios"
          }
        },
        "android": {
          "executor": "@nativescript/nx:build",
          "options": {
            "platform": "android"
          }
        },
        "clean": {
          "executor": "@nativescript/nx:build",
          "options": {
            "clean": true
          }
        }
      }
    },
    "all": {
      "root": "",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages"
          ],
          "options": {
            "commands": [
              "nx run nativescript-image-cache:build.all",
              "nx run nativescript-searchable-select:build.all",
              "nx run nativescript-sewoo-printer:build.all",
              "nx run nativescript-woosim-printer:build.all",
              "nx run nativescript-intermec-printer:build.all",
              "nx run nativescript-socketmobile:build.all",
              "nx run nativescript-t3b-printer:build.all"
            ],
            "parallel": false
          }
        },
        "focus": {
          "outputs": [
            "dist/packages"
          ],
          "options": {
            "commands": [
              "nx g @nativescript/plugin-tools:focus-packages"
            ],
            "parallel": false
          },
          "executor": "@nrwl/workspace:run-commands"
        }
      },
      "sourceRoot": ""
    },
    "nativescript-image-cache": {
      "root": "packages/nativescript-image-cache",
      "projectType": "library",
      "sourceRoot": "packages/nativescript-image-cache",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/nativescript-image-cache",
            "tsConfig": "packages/nativescript-image-cache/tsconfig.json",
            "packageJson": "packages/nativescript-image-cache/package.json",
            "main": "packages/nativescript-image-cache/index.d.ts",
            "assets": [
              "packages/nativescript-image-cache/*.md",
              "packages/nativescript-image-cache/index.d.ts",
              "LICENSE",
              {
                "glob": "**/*",
                "input": "packages/nativescript-image-cache/platforms/",
                "output": "./platforms/"
              }
            ]
          }
        },
        "build.all": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx run nativescript-image-cache:build",
              "node tools/scripts/build-finish.ts nativescript-image-cache"
            ],
            "parallel": false
          }
        },
        "focus": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx g @nativescript/plugin-tools:focus-packages nativescript-image-cache"
            ],
            "parallel": false
          }
        }
      }
    },
